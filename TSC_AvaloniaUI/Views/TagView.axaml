<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:TSC_AvaloniaUI.ViewModels"
             xmlns:controls="clr-namespace:TSC_AvaloniaUI.Controls"
             x:DataType="vm:TagViewModel"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="TSC_AvaloniaUI.Views.TagView">
    <controls:TagButton
        ButtonCommand="{Binding RemoveTagCommand}"
        ButtonCommandParameter="{Binding .}"
        TagType="{Binding TagType}"
        TagText="{Binding Name}"/>
</UserControl>
