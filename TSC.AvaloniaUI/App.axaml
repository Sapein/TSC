<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="TSC.AvaloniaUI.App"
             xmlns:local="using:TSC.AvaloniaUI"
             RequestedThemeVariant="Dark">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="avares://TSC.AvaloniaUI/Icons.axaml" />
        <StyleInclude Source="avares://TSC.AvaloniaUI/Controls/TagButton.axaml" />
    </Application.Styles>
</Application>